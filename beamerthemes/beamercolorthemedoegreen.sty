\ProvidesPackageRCS $Header: /Users/joseph/Documents/LaTeX/beamer/base/themes/color/beamercolorthemeflame.sty,v d02a7cf4d8ae 2010/06/17 09:11:41 rivanvx $

\definecolor{doegreen}{RGB}{33, 99, 50}
\definecolor{uicred}{RGB}{209, 49, 55}

\mode<presentation>

\setbeamercolor{title}{fg=white,bg=doegreen}
\setbeamercolor{subtitle}{fg=white,bg=doegreen}
\setbeamercolor{author}{fg=doegreen}
\setbeamercolor{date}{fg=doegreen}

\setbeamercolor{structure}{fg=doegreen}

\setbeamercolor{block title}{use=structure,fg=white,bg=structure.fg!75!black}
\setbeamercolor{block title alerted}{use=alerted text,fg=white,bg=alerted text.fg!75!black}
\setbeamercolor{block title example}{use=example text,fg=white,bg=example text.fg!75!black}


\setbeamercolor{frametitle}{use=structure,fg=doegreen,bg=white}
\setbeamercolor{frametitle underline}{use=structure,fg=black,bg=doegreen}

\setbeamercolor{normal text}{use=structure,fg=doegreen}

\setbeamercolor{block body}{parent=normal text,use=block title,bg=block title.bg!10!bg,fg=doegreen}
\setbeamercolor{block body alerted}{parent=normal text,use=block title alerted,bg=block title alerted.bg!10!bg}
\setbeamercolor{block body example}{parent=normal text,use=block title example,bg=block title example.bg!10!bg}

\mode
<all>
